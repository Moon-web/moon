.loading
  position fixed
  top 0
  left 0
  right 0
  bottom 0
  background-color rgba(0,0,0,0.3)
  z-index 9999
  .loading-wrapper
    position absolute
    width 80px
    height 80px
    left 50%
    top 50%
    margin-top -40px
    margin-left -40px
    background-color #fff
    background-repeat no-repeat
    background-position center
    border-radius 3px
    background-image url("resource/laoding_1.gif")
