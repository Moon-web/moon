@import "../../common/common.styl"
.register-topcon
  width 100%
  height 160px
  box-sizing border-box
  border-bottom 1px solid #ededed
  padding 20px 0
  .register-top
    width 1200px
    height 100%
    margin 0 auto
    .register-top-left
      float left
      width 800px
      overflow hidden
      .register-topLeft
        width 260px
        height 120px
        cursor pointer
        float left
        background url("resource/LOGO.png") no-repeat center
        border-right 1px solid #ededed
      .register-topRight
        float left
        height 100%
        padding-left 50px
        line-height 120px
    .register-top-right
      float right
      line-height 120px
      mainColor()
      cursor pointer
