.register-out
  width 100%
  height 100%
  position fixed
  left 0px
  top 0px
  background rgba(0,0,0,0.4)
  z-index 999
  display none
  .registerOut
    width 400px
    height 500px
    position absolute
    left 50%
    top 50%
    margin-top -250px
    margin-left -200px
    font-size 14px
    color #999
    box-shadow 0px 0px 5px #333
    padding 10px
    box-sizing border-box
    background #fff
    .register-close
      width 100%
      height 30px
      font-size 22px
      text-align right
      line-height 30px
      padding-right 10px
      box-sizing border-box
      display block
      cursor pointer
      background url("resource/close.png") no-repeat right center
    #registerOutCon
      width 100%
      height 450px
      overflow-y auto


.OutIsShow
