  .phone-register
    width 1200px
    height auto
    margin 0 auto
    .register-con
      width 500px
      margin 0 auto
      height auto
      .information
        height 36px
        margin-top 30px
        width 100%
        input::-webkit-outer-spin-button
        -webkit-appearance none
        input::-webkit-inner-spin-button
        -webkit-appearance none
        input[type="number"]
          -moz-appearance textfield
        .title
          float left
          width 115px
          height 100%
          text-align left
          line-height 36px
          &.necessary
            &:before
              content '*'
              color #f00
              iBlock()
              margin-top 3px
              margin-right 3px
        .phone
          float left
          height 100%
          width 385px
          padding-left 10px
          box-sizing border-box
          border 1px solid #cccccc
          outline none
          font-size 16px
        .code
          width 240px
          float left
          margin-right 15px
          padding-left 10px
          height 100%
          box-sizing border-box
          border 1px solid #cccccc
          outline none
        .yanCode
          margin-left 5px
          display block
          padding-top 45px
          text-align center
          font-size 14px
        .codeImg
          float left
          height 100%
          width 130px
        .address
          float left
          height 100%
          width 385px
        .submit
          width 130px
          height 36px
          float left
          line-height 36px
          color #ffffff
          border none
          text-align center
          border-radius 4px
          &.on
            mainBack()
            cursor pointer
          &.off
            background #e1e1e1
        .blue
          color #0099ff
          &:hover
            mainColor()
            cursor pointer
        .vmiddle
          float left
          margin-left 115px
          margin-right 10px
          -webkit-appearance checkbox
      .confirm
        height 45px
        margin-top 15px
        .confirm-con
          float left
          width 280px
          height 100%
          margin-left 115px
          .commit
            width 100%
            height 100%
            mainBack()
            text-align center
            line-height 36px
            color #fff
            border none
            border-radius 4px
            cursor pointer
            font-size 16px
      .find
        height 36px
        margin-top 20px
        .find-con
          float left
          height 100%
          width 385px
          margin-left 115px
          .haveAccount
            float left
            height 100%
            line-height 36px
            margin-right 90px
            cursor pointer
            &:hover
              mainColor()
              cursor pointer
          .findpw
            float right
            height 100%
            line-height 36px
            color #0099ff
            &:hover
              mainColor()
              cursor pointer
