@import "../../common/common.styl"
.swiper-wrapper
  width 100%
  height 440px
  position relative
  .banner-wrapper
    position absolute
    top 0
    left 0
    height 100%
    width 100%
    z-index -1
    overflow hidden
    .banner-content
      width 1200px
      height 100%
      margin 0 auto
      .banner-box
        width 100%
        height 100%
        position relative
        left -360px
        top 0
        .banner-contentBox
          width 1920px
          height 100%
          .background-item
            iBlock()
            width 100%
            height 100%
            background-position center
            background-size cover
            background-repeat no-repeat
          .background-item[lazy=loading]
            background-size auto
          .background-item[lazy=error]
            background-size auto
.bidingList-container
  width 100%
  height auto
  background #f5f5f5
  .bidList-content
    width 1200px
    height auto
    overflow hidden
    margin 0 auto
    padding 30px 0 60px 0
    box-sizing border-box
    .bidList-left
      float left
      width 880px
      height auto
      margin-right 42px
      .bidList-left-top
        width 100%
        height 167px
        background url("resource/mian_1.png") no-repeat right
      .bidList-left-bottom
        width 100%
        padding 20px 0 0 12px
        box-sizing border-box
        height auto
        .bidList-list
          width 100%
          height 200px
          border 1px solid #e1e1e1
          box-sizing border-box
          margin-bottom 20px
          background #fff
          padding 18px 0
          .bidList-box
            width 100%
            height 100%
            .bidList-list-left
              float left
              height 100%
              width 515px
              border-right 1px solid #e1e1e1
              box-sizing border-box
              padding-left 24px
              position relative
              .bidList-list-title
                position relative
                z-index 1
                width 100%
                height 46px
                line-height 46px
                font-size 20px
                color #333333
                .bidList-list-name
                  float left
                  height 100%
                  margin-right 25px
                .bidList-list-number
                  float left
                  height 100%
              .bidList-price
                position relative
                z-index 1
                width 100%
                height 36px
                line-height 36px
                color #666
                .bidList-list-name
                  float left
                  height 100%
                  padding-right 5px
                  box-sizing border-box
                  width 210px
                  overflow hidden
                  text-overflow ellipsis
                  white-space nowrap
                  font-size 14px
                  .bidList-list-number
                    height 100%
                    font-size 18px
                    color #ff9900
                    font-weight bold
                .time
                  float left
                  height 100%
                  width 270px
                  overflow hidden
                  text-overflow ellipsis
                  white-space nowrap
                  font-size 14px
                  .begin-time
                    float left
                    height 100%
                    margin-right 5px
                  .lastTime
                    float left
                    height 100%
                    font-size 16px
                    font-weight bold
                    color #ff6a00
              .bidList-num
                position relative
                z-index 1
                width 100%
                height 40px
                line-height 40px
                font-size 14px
                color #666
                .bidList-list-name
                  float left
                  height 100%
                .bidList-list-number
                  float left
                  height 100%
              .bidList-time
                width 100%
                position relative
                z-index 1
                height 40px
                line-height 40px
                font-size 14px
                color #666
                .bidList-list-name
                  float left
                  height 100%
                .bidList-list-number
                  float left
                  height 100%
              .over-bg
                position absolute
                right -15px
                top 7px
                width 181px
                height 147px
                background url("resource/over.png") no-repeat center
                z-index 0
            .bidList-list-right
              float left
              height 100%
              width 350px
              .bidList-list-city
                width 100%
                height 40px
                line-height 40px
                text-align center
                font-size 18px
                color #333
              .bidList-list-sureMoney
                width 100%
                height 46px
                line-height 46px
                text-align center
                font-size 14px
                color #666
              .bidList-list-startTime
                width 100%
                height 42px
                line-height 42px
                text-align center
                font-size 14px
                color #666
                margin-top 20px
                .detail-time
                  font-weight bold
                  font-size 18px
                  color #ff6a00
                .biding
                  height 100%
                  display block
                  width 210px
                  margin 0 auto
                  mainBack()
                  color #fff
                  text-align center
                  cursor pointer
                  font-size 18px
          .over-bidlist
            .bidList-list-city
              color #999
            .bidList-list-sureMoney
              color #999
            .bidList-list-startTime
              .biding
                background #ccc
                cursor default
    .bidList-right
      float left
      width 278px
      height auto
      padding-top 7px
      box-sizing border-box
      .other-top
        width 100%
        height 215px
        padding-top 18px
        box-sizing border-box
        margin-bottom 10px
        background #fff
        border 1px solid #e1e1e1
        .other-top-bg
          width 100%
          height 70px
          background url("resource/unlogin.png") no-repeat center
        .other-top-content
          display block
          width 100%
          text-align center
        .other-infomation
          width 100%
          margin-top 16px
          height auto
          overflow hidden
          .login-con
            width 100%
            height auto
            overflow hidden
            .statue
              mainBack()
              color #fff
            .register
              float left
              height 30px
              width 94px
              text-align center
              line-height 28px
              box-sizing border-box
              mainBorder(1px,solid)
              cursor pointer
              font-size 14px
            .login
              float left
              height 30px
              width 94px
              text-align center
              line-height 28px
              margin 0 10px 0 38px
              font-size 14px
              box-sizing border-box
              mainBorder(1px,solid)
              cursor pointer
          .exit-login-con
            width 100%
            height auto
            overflow hidden
            .exit-login
              display block
              height 30px
              width auto
              text-align center
              line-height 28px
              box-sizing border-box
              cursor pointer
              font-size 14px
              margin 0 auto
        .other-small-item
          width 100%
          height auto
          overflow hidden
          padding 16px 20px 0 38px
          box-sizing border-box
          .small-item
            float left
            width 30px
            height 30px
            margin-right 26px
            &:last-child
              margin-right 0px
              background url("resource/icon_m_4.png") no-repeat center
            &:nth-child(1)
              background url("resource/icon_m_1.png") no-repeat center
            &:nth-child(2)
              background url("resource/icon_m_2.png") no-repeat center
            &:nth-child(3)
              background url("resource/icon_m_3.png") no-repeat center
      .bidList-success
        width 100%
        height auto
        margin-top 20px
        box-sizing border-box
        background #fff
        border 1px solid #e1e1e1
        .success-title
          width 100%
          height 57px
          line-height 60px
          text-align center
          color #333333
          font-size 24px
        .success-list
          width 100%
          padding 0 20px 20px 20px
          box-sizing border-box
          height 320px
          .success-item
            height 32px
            line-height 32px
            font-size 12px
            .success-item-con
              color #333333
              float left
              height 100%
              width 75%
              margin-right 10%
              overflow hidden
              text-overflow ellipsis
              white-space nowrap
              cursor pointer
              &:hover
                mainColor()
            .success-time
              float left
              height 100%
              color #f90
              width 15%
