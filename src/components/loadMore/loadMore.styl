@import "../../common/common.styl"
.load
  position relative
  width 100%
  height 70px
  text-align center
  line-height 50px
  font-weight 300
  font-size 14px
  cursor pointer
  mainColor()
  &.loadOver
    color #e1e1e1
  .loading
    position absolute
    top 0
    left 0
    right 0
    bottom 0
    background-color #f5f5f5
    color #333333
    text-align center
    line-height 50px
    z-index 10
