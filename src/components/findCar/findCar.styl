@import "../../common/common.styl"
.findCar-content
  width 1200px
  height auto
  margin 0 auto
  .findCar-title
    height 60px
    line-height 80px
    font-size 14px
    .title
      width 100%
      height 100%
      .titleList
        color #333
  .find
    width 100%
    height 100px
    border 1px solid #e5e5e5
    margin-top 25px
    box-sizing border-box
    .address
      float left
      margin-top 32px
      height 32px
      width 342px
      margin-left 22px
      p
        color #333333
        font-size 14px
        .begin
          height 32px
          width 110px
          border 1px solid #e5e5e5
          margin 0 10px
          padding 0
          padding-left 5px
          box-sizing border-box
        .end
          height 32px
          width 110px
          border 1px solid #e5e5e5
          margin 0 10px
          padding 0
          padding-left 5px
          box-sizing border-box
    .dateTime
      float left
      margin-top 32px
      height 32px
      .block
        margin 0 30px
        color #333
        font-size 14px
        line-height 32px
        input
          height 32px
          width 160px
          border 1px solid #e5e5e5
          margin 0 10px
          padding 0
          padding 5px
          box-sizing border-box
          color #333
          cursor default
          background url("resource/icon_calendar.png") no-repeat 135px center
        i
          display none
    .num
      float left
      margin-top 32px
      height 32px
      p
        margin-right 10px
        color #333
        line-height 32px
        font-size 14px
        .limit
          height 32px
          width 116px
          border 1px solid #e5e5e5
          margin 0 10px
          padding 0
          padding-left 5px
          box-sizing border-box
        .max
          height 32px
          width 116px
          border 1px solid #e5e5e5
          margin 0 10px
          padding 0
          padding-left 5px
          box-sizing border-box
    .button
      float left
      margin-top 32px
      height 32px
      .button-search
        width 100px
        height 32px
        border none
        background #2b9cf0
        color #fff
        border-radius 5px
        cursor pointer
  .carCenter
    width 100%
    height auto
    margin-bottom 20px
    .findCar
      width 102%
      padding 20px 0
      overflow hidden
      box-sizing border-box
      .carBox
        width 400px
        height 232px
        border 1px solid #e5e5e5
        margin 0px -1px -1px 0
        padding 0 25px
        position relative
        float left
        box-sizing border-box
        .title
          color #666666
          margin-top 10px
          height 50px
          line-height 50px
          .start
            height 50px
            line-height 50px
            float left
            font-size 18px
          .bg
            float left
            margin 14px 15px
            width 50px
            height 22px
            background url("resource/findCar.png") no-repeat center
          .end
            height 50px
            line-height 50px
            float left
            font-size 18px
        .goodsW
          line-height 27px
          color #666666
          font-size 14px
          .goodsNum
            mainColor()
        .beginTime
          line-height 27px
          color #666666
          font-size 14px
        .explain
          line-height 27px
          color #666666
          font-size 14px
        .useCar
          text-align right
          position absolute
          height 35px
          width 400px
          bottom 20px
          right 25px
          font-size 14px
          button
            width 100px
            height 35px
            border none
            background #2b9cf0
            color #fff
            border-radius 5px
            outline none
            cursor pointer
  .page-wrapper
    margin-bottom 30px
    .page-content
      width 1180px
      height 24px
      text-align center
      font-size 0
      .changePage
        display inline-block
        vertical-align top
        width auto
        height 100%
        padding 0 5px
        font-size 12px
        color #fff
        mainBack()
        text-align center
        line-height 24px
        border-radius 1px
        cursor pointer
        &.end
          background #e9e9e9
          color #666
          border-radius 1px
          cursor default
      .pageFlag
        display inline-block
        vertical-align top
        padding 0 10px
        font-size 12px
        color #999
        line-height 24px
      .linkPage
        display inline-block
        vertical-align top
        width auto
        height 100%
        .entry-wrapper
          display inline-block
          vertical-align top
          width 50px
          height 100%
          box-sizing border-box
          border 1px solid #e1e1e1
          margin-left 20px
          .entry
            width 100%
            height 100%
            border none
            text-align center
        .tag
          display inline-block
          vertical-align top
          padding 0 5px
          line-height 24px
          color #999
          font-size 12px
          cursor default
        .confirm
          display inline-block
          height 100%
          padding 0 10px
          mainBack()
          color #fff
          font-size 12px
          line-height 24px
          margin-left 15px
          border-radius 1px
          cursor pointer

