.citySelect
  width 100%
  height 100%
  font-size 0px
  .select
    display inline-block
    vertical-align top
    position relative
    margin-right 5px
    &:last-child
      margin-right 0
    .selected
      width auto
      box-sizing border-box
      border 1px solid #e1e1e1
      font-size 12px
      color #333




