.bidenroll-container
  width 1200px
  height 650px
  margin 50px auto
  box-sizing border-box
  border 1px solid #e6e6e6
  padding 0 10px
  .bidenroll-title
    width 100%
    padding-left 10px
    box-sizing border-box
    line-height 62px
    font-size 16px
    font-weight bold
    color #333
  .bidenroll-content
    width 100%
    height auto
    .bidenroll-content-title
      height 48px
      box-sizing border-box
      border 1px solid #e6e6e6
      line-height 46px
      overflow hidden
      background #f0fff1
      font-size 14px
      color #808080
      margin-bottom 20px
      .bidenroll-content-item
        float left
        height 100%
        text-align center
        overflow hidden
        text-overflow ellipsis
        white-space nowrap
        &:nth-child(1)
          width 20%
        &:nth-child(2)
          width 40%
        &:nth-child(3)
          width 20%
        &:nth-child(4)
          width 20%
    .bidenroll-content-box
      box-sizing border-box
      border 1px solid #e6e6e6
      height 120px
      line-height 120px
      width 100%
      .bidenroll-content-item
        float left
        height 100%
        text-align center
        overflow hidden
        text-overflow ellipsis
        white-space nowrap
        font-size 14px
        color #333
        &:nth-child(1)
          width 20%
        &:nth-child(2)
          width 40%
        &:nth-child(3)
          width 20%
        &:nth-child(4)
          width 20%
        .bidenroll-content-item-bg
          float left
          width 66px
          height 66px
          margin 27px 20px 27px 100px
          background-position center
          background-size contain
          background-repeat no-repeat
        .bidenroll-content-item-bg[lazy=error]
          background-size contain
        .bidenroll-content-item-bg[lazy=loading]
          background-size contain
        .bidenroll-content-item-con
          float left
          height 100%
          width 270px
          text-align left
          overflow hidden
          text-overflow ellipsis
          white-space nowrap
          line-height 120px
          font-size 14px
          color #333
        .bidenroll-content-item-num
          display block
          width 100px
          height 40px
          border 1px solid #e6e6e6
          margin 40px auto
          line-height 40px
          text-align center
          font-size 14px
          color #333
    .bidenroll-content-address
      height 72px
      width 100%
      border 1px solid #e6e6e6
      box-sizing border-box
      margin-top 20px
      .address-title
        float left
        line-height 70px
        margin-right 20px
        padding-left 40px
        box-sizing border-box
        font-size 14px
        color #333
      .detail-address
        float left
        margin 20px 0 0 0
  .bidenroll-content-bid
    width 100%
    height auto
    .bidenroll-content-bid-con
      width 100%
      overflow hidden
      .bidenroll-content-bid-right
        float right
        width 290px
        margin-right 60px
        padding 35px 0
        box-sizing border-box
        overflow hidden
        .nim-num
          width 100%
          font-size 14px
          color #666
          height 38px
          line-height 38px
          .title-item
            float left
            width 100px
            text-align right
          .title-con
            float left
            width 190px
            text-align right
            padding-left 20px
            box-sizing border-box
        .enroll-money
          width 100%
          font-size 14px
          color #666
          height 38px
          line-height 38px
          .title-item
            font-size 14px
            width 100px
            text-align right
            float left
            color #333333
            font-weight bold
          .title-con
            float left
            font-size 24px
            color #f35833
            font-weight bold
            width 190px
            text-align right
  .enroll
    width 100%
    height 54px
    border 1px solid #e6e6e6
    box-sizing border-box
    .enroll-left
      height 100%
      float left
      line-height 52px
      font-size 14px
      padding-left 36px
      box-sizing border-box
      font-size 14px
      color #333
      .xieyi
        color #2b9cf0
        cursor pointer
    .enroll-right
      float right
      width 360px
      position relative
      height 52px
      .enroll-cancle
        position absolute
        left 0px
        top -1px
        width 180px
        height 54px
        text-align center
        line-height 54px
        font-size 18px
        background #ccc
        color #fff
        cursor pointer
      .enroll-button
        height 54px
        width 180px
        text-align center
        line-height 54px
        font-size 18px
        background #f35833
        color #fff
        cursor pointer
        position absolute
        right 0px
        top -1px

